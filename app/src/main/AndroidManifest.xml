<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ylw.surfacetest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".CircleActivity">
        </activity>
        <activity android:name=".FiveActivity">
        </activity>
        <activity
            android:name=".SurfaceActivity"
            android:label="@string/title_activity_surface"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".AnimationActivity"
            android:label="@string/title_activity_animation"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>
